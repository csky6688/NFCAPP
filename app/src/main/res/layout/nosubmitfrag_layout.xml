<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFEFEF"
    android:id="@+id/nosubmit_main"
    >


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:id="@+id/history_Relayout"
        android:orientation="horizontal"
        android:background="#304D8F"
        >
        <ImageView
            android:visibility="gone"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/back_image"
            android:padding="3dp"
            android:layout_centerVertical="true"
            android:src="@drawable/back"
            android:onClick="back"
            />
        <TextView
            android:id="@+id/show_user"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="未提交"
            android:textSize="20sp"
            android:textColor="@android:color/white"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:orientation="horizontal"
            android:background="#2BB310"
            android:layout_weight="1.5"
            >
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:orientation="horizontal"
                android:background="#2BB310"
                android:layout_weight="3.3">
                <TextView
                    android:id="@+id/nosubmit_data"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#FAFAFA"
                    android:textSize="17sp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:orientation="horizontal"
                android:background="#2BB310"
                android:layout_weight="1">

                <ImageView
                    android:layout_marginLeft="10dp"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:src="@drawable/down"
                    android:layout_gravity="center_vertical"
                    />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="#FAFAFA"
            android:orientation="horizontal"
            >
            <TextView
                android:id="@+id/shuju"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="数据:"
                android:textColor="#304D8F"
                android:textSize="17sp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                />
            <TextView
                android:id="@+id/no_submit_count"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#304D8F"
                android:textSize="17sp"
                android:layout_marginLeft="5dp"
                android:layout_gravity="center_vertical"
                />
        </LinearLayout>
    </LinearLayout>
    <ImageView
        android:visibility="gone"
        android:id="@+id/fail_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/pre_no"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="90dp"
        />
    <TextView
        android:visibility="gone"
        android:id="@+id/fail_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="没有找到未提交的记录"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="10dp"
        />
    <LinearLayout
        android:id="@+id/no_sublist"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FAFAFA"
        android:layout_marginTop="0.5dp"
        android:orientation="vertical"
        >
        <View
            android:visibility="gone"
            android:id="@+id/subview"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#C6C6C6"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            />

        <ListView
            android:id="@+id/nosubmit_listView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:divider="#C6C6C6"
            android:dividerHeight="0.5dp"></ListView>
        <View
            android:visibility="gone"
            android:id="@+id/subview2"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#C6C6C6"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            />
    </LinearLayout>


</LinearLayout>